{
    "default-features": [
        "wslay",
        "curl"
    ],
    "dependencies": [
        "protobuf",
        "rapidjson",
        "optional-lite"
    ],
    "features": {
        "wslay": {
            "description": "Websockets library decoupled from IO and handshakes.",
            "dependencies": [
                {
                    "name": "wslay",
                    "platform": "linux | osx | ios"
                }
            ]
        },
        "curl": {
            "description": "The libcurl networking library.",
            "dependencies": [
                {
                    "name": "curl",
                    "default-features": false,
                    "features": [
                        "ssl"
                    ],
                    "platform": "linux | osx | ios"
                }
            ]
        },
        "libcxx": {
            "description": "An alternative C++ standard library, used by Unreal Engine.",
            "dependencies": [
                {
                    "name": "llvm",
                    "default-features": false,
                    "features": [
                        "libcxx"
                    ],
                    "platform": "linux"
                }
            ]
        },
        "grpc": {
            "description": "The grpc networking protocol.",
            "dependencies": [
                "grpc"
            ]
        }
    }
}